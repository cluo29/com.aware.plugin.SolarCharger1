        <provider android:name="com.aware.plugin.SolarCharger1.Provider" android:authorities="${applicationId}.provider.SolarCharger1" android:exported="true" android:readPermission="com.aware.READ_CONTEXT_DATA" android:writePermission="com.aware.WRITE_CONTEXT_DATA"/>
        <activity
            android:name=".Settings"
            android:label="@string/app_name"
            android:exported="true" >
        </activity>
    </application>
</manifest>
